{
  "name": "drupal/protect_before_launch",
  "type": "drupal-module",
  "description": "Protect your site before launch with HTTP basic authentication",
  "keywords": ["drupal", "protect", "auth", "authentication", "basic auth", "env", "environment"],
  "license": "GPL-2.0+",
  "homepage": "https://www.drupal.org/project/protect_before_launch",
  "authors": [
    {
      "name": "Jason Lentink (monkjason)",
      "homepage": "https://www.drupal.org/u/monkjason",
      "role": "Maintainer"
    },
    {
      "name": "Robert Slootjes",
      "email": "robert@mediamonks.com",
      "homepage": "https://github.com/slootjes"
    }
  ],
  "require": {
    "php": "^5.6|^7.0"
  },
  "require-dev": {
    "drupal/console": "^1.0",
    "mockery/mockery": "^0.9.9"
  },
  "suggest": {
    "drupal/console": "Allow status, username and password to be set by command line interface"
  }
}
